<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="panama_tax_report_wizard_form" model="ir.ui.view">
			<field name="name">panama.tax.report.wizard.form</field>
			<field name="model">panama.tax.report.wizard</field>
			<field name="arch" type="xml">
				<form string="">
					<sheet>
						<group>
							<group>
								<field name="start_date"/>
								<field name="end_date"/>
							</group>
						</group>
						<footer>
							<button name="Confirm" string="Confirmar" type="object" class="oe_highlight"/>
							<button string="Cancel" class="btn btn-secondary" special="cancel" />
						</footer>
					</sheet>
				</form>
			</field>
		</record>
		
	</data>
</odoo>
