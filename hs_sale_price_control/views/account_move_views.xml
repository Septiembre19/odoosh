<?xml version='1.0' encoding='utf-8'?>
<odoo>
	<data>
		
		<record id="account_move_inherit_form" model="ir.ui.view">
			<field name="name">account.move.inherit.form</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='sequence']" position="after">
					<field name="manage_price" invisible="1"/>
				</xpath>
				<xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_unit']" position="attributes">
					<attribute name="attrs">{'readonly':[('manage_price', '!=', True)]}</attribute>
				</xpath>
			</field>
		</record>
		
	</data>
</odoo>