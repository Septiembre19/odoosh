<templates>

	<t t-name="QtyDetailPopOver" t-inherit="sale_stock.QtyDetailPopOver">
		<xpath expr="//table/tbody/t[1]/tr[2]" position="after">
			<tr><td><strong>Other</strong><br /></td><td></td></tr>
				<t t-foreach="data.alt_qty_available" t-as="line">
					<tr>
						<td><small><t t-esc='line.company'/></small></td>
						<td>
							<b t-esc='line.quantity'/>
							<t t-esc='data.product_uom.data.display_name'/>
						</td>
					</tr>
				</t>
		</xpath>
	</t>

</templates>